{
  "name": "@pgql/faker",
  "version": "0.4.6",
  "description": "faker helpers",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "bundle": "lql package",
    "copy": "cpy ../../../LICENSE README.md package.json dist --flat",
    "clean": "rimraf dist",
