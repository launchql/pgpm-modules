{
  "name": "@pgql/jobs",
  "version": "0.4.6",
  "description": "jobs",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "copy": "cpy ../../../LICENSE README.md package.json dist --flat && cpy \"schemas/**\" \"sql/**\" \"*.control\" \"launchql.plan\" dist --parents --cwd . && sh -c 'for d in deploy verify revert; do if [ -d \"$d\" ]; then cp -a \"$d\" dist/; fi; done'",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "build": "npm run clean && npm run copy",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch"
  },
  "dependencies": {
    "@pgql/default-roles": "workspace:^",
    "@pgql/jwt-claims": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/extensions"
  },
  "homepage": "https://github.com/launchql/extensions",
  "bugs": {
    "url": "https://github.com/launchql/extensions/issues"
  }
}
